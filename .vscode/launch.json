{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Extension starten",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}"
            ],
            "stopOnEntry": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/out",
            "preLaunchTask": "npm"
        },
        {
            "name": "Build",
            "type": "miengine",
            "request": "launch",
            "launchOptionType": "Local",
            "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
            "targetArchitecture": "x64",
            "program": "Gdip_ImageSearch.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "processId": "1500"
        }
    ]
}